extends layout

block content
  div.container
      form
        .form-group
          label(for='InputEmail') Email address
          input#InputEmail.form-control(type='email', aria-describedby='emailHelp', placeholder='Enter email')
        .form-group
          label(for='InputPhoneNum') Phone Number
          input#InputPhoneNum.form-control(type='text', placeholder='Phone Number')
        button.btn.btn-primary(type='submit') Submit
